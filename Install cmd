TMC Autotune 
wget -qO - https://raw.githubusercontent.com/andrewmcgr/klipper_tmc_autotune/main/install.sh | sed 's/klipper\.service/klipper-K1.service/g' | bash

ShakerTune
wget -qO - https://raw.githubusercontent.com/Frix-x/klippain-shaketune/main/install.sh | sed 's/klipper\.service/klipper-K1.service/g' | bash

Cartographer
cd ~
git clone https://github.com/Cartographer3D/cartographer-klipper.git
./cartographer-klipper/install.sh

